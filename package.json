  "license": "MIT",
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://github.com/danielroe"
  },
  "exports": {
    ".": "./dist/module.mjs"
  },
  "type": "module",
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": ["./dist/types.d.mts"]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
