    "ufo": "^1.5.4"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "1.13.0",
    "@nuxt/module-builder": "1.0.2",
    "@nuxt/schema": "4.3.1",
    "@nuxt/test-utils": "4.0.0",
    "@types/serve-static": "2.2.0",
    "@vitest/coverage-v8": "4.0.18",
    "bumpp": "10.4.0",
    "eslint": "9.39.2",
    "nuxt": "4.3.1",
    "typescript": "5.9.3",
    "vitest": "4.0.18",
    "vue": "3.5.27"
