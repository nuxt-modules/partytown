    "test": "vitest run --coverage",
    "release": "bumpp && npm publish"
  },
  "dependencies": {
    "@nuxt/kit": "^4.0.0",
    "@qwik.dev/partytown": "^0.11.0",
    "knitwork": "^1.2.0",
